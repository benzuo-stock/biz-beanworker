{
    "name": "benzuo-stock/biz-beanworker",
    "description": "A beanstalkd-based queue worker framework.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "BeanWorker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "TestProject\\": "tests/TestProject/src/"
        }
    },
    "require": {
        "php": "^7.1",
        "pimple/pimple": "^3.0",
        "monolog/monolog": "^1.0",
